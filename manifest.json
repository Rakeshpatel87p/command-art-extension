{
  "manifest_version": 2,
  "name": "Command Art",
  "description": "Art in your new tab page.",
  "version": "1.2",
  "chrome_url_overrides": {
    "newtab": "page.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "ORIGIN"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
